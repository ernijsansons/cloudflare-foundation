{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "foundation-agents",
  "main": "src/index.ts",
  "compatibility_date": "2025-09-25",
  "compatibility_flags": ["nodejs_compat"],
  "observability": { "enabled": true },
  "durable_objects": {
    "bindings": [
      { "name": "CHAT_AGENT", "class_name": "ChatAgent" },
      { "name": "TASK_AGENT", "class_name": "TaskAgent" },
      { "name": "TENANT_AGENT", "class_name": "TenantAgent" },
      { "name": "SESSION_AGENT", "class_name": "SessionAgent" },
      { "name": "MCP_SERVER", "class_name": "FoundationMcpServer" },
      { "name": "RATE_LIMITER", "class_name": "TenantRateLimiter" }
    ]
  },
  "migrations": [
    { "tag": "v1", "new_sqlite_classes": ["ChatAgent", "TaskAgent", "TenantAgent", "SessionAgent", "FoundationMcpServer", "TenantRateLimiter"] }
  ],
  "ai": { "binding": "AI" },
  "d1_databases": [
    { "binding": "DB", "database_name": "foundation-primary", "database_id": "34bce593-9df9-4acf-ac40-c8d93a7c7244" }
  ],
  "r2_buckets": [
    { "binding": "FILES", "bucket_name": "foundation-files" }
  ],
  "kv_namespaces": [
    { "binding": "CACHE_KV", "id": "ef2305fbf6da4cffa948193efd40f40c" }
  ],
  "vectorize": [
    { "binding": "VECTOR_INDEX", "index_name": "foundation-embeddings" }
  ],
  "queues": {
    "producers": [
      { "binding": "AUDIT_QUEUE", "queue": "foundation-audit" },
      { "binding": "ANALYTICS_QUEUE", "queue": "foundation-analytics" }
    ]
  },
  "analytics_engine_datasets": [
    { "binding": "ANALYTICS", "dataset": "foundation_events" }
  ],
  "env": {
    "staging": {
      "name": "foundation-agents-staging",
      "d1_databases": [
        { "binding": "DB", "database_name": "foundation-primary-staging", "database_id": "STAGING_DB_ID" }
      ],
      "r2_buckets": [
        { "binding": "FILES", "bucket_name": "foundation-files-staging" }
      ],
      "kv_namespaces": [
        { "binding": "CACHE_KV", "id": "STAGING_KV_ID" }
      ],
      "vectorize": [
        { "binding": "VECTOR_INDEX", "index_name": "foundation-embeddings-staging" }
      ]
    },
    "production": {
      "name": "foundation-agents-production",
      "d1_databases": [
        { "binding": "DB", "database_name": "foundation-primary", "database_id": "34bce593-9df9-4acf-ac40-c8d93a7c7244" }
      ],
      "r2_buckets": [
        { "binding": "FILES", "bucket_name": "foundation-files" }
      ],
      "kv_namespaces": [
        { "binding": "CACHE_KV", "id": "ef2305fbf6da4cffa948193efd40f40c" }
      ],
      "vectorize": [
        { "binding": "VECTOR_INDEX", "index_name": "foundation-embeddings" }
      ]
    }
  }
}
